@model IEnumerable<CAG_DW.Models.Agreement>

@{
    ViewData["Title"] = "Index";
}

<h1>Agreements</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OrderDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AgreementID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AgreementID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AgreementID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
